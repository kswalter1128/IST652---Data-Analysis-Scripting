iter.","company_description":"With over 840+ locations, we proudly provide jobs to more than 13,000 team members in the US and Canada. Fueled by industry-leading growt...","job_board":"indeed","geo":"usa","job_post_lang":"en","site_name":"indeed_usa_com_job_board_common_setup_pc_jobspikr","domain":"indeed_com","postdate_yyyymmdd":"20190803","postdate_in_indexname_format":"2019.08.01","inferred_city":"Irving","inferred_state":"Texas","inferred_country":"United states","fitness_score":10}
